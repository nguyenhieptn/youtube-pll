{
  "version": "1.0.0",
  "manifest_version": 2,
  "name": "Youtube PLL",
  "description": "A tool for get or set cookie for tool Youtube PLL",
  "permissions": [
    "cookies",
    "proxy",
    "tabs",
    "unlimitedStorage",
    "storage",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_title": "Youtube PLL",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "config.js",
      "background.js"
    ]
  },
  "icons": {
    "16": "/icon_16.png",
    "48": "/icon_48.png",
    "128": "/icon_128.png"
  },
  "minimum_chrome_version": "22.0.0"
}